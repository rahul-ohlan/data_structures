    "import sys\n",
    "sys.setrecursionlimit(5000)\n",
    "def fact(n):\n",
    "    if n==0:\n",
    "        return 1\n",
    "    return n*fact(n-1)\n",
    "n = int(input())\n",
    "print(fact(n))"
