   "source": [
    "from sys import setrecursionlimit\n",
    "import queue\n",
    "setrecursionlimit(10**6)\n",
    "\n",
    "def reverseQueue(inputQueue):\n",
    "    if inputQueue.qsize == 1:\n",
    "        return\n",
    "    first = inputQueue.get()\n",
    "    reverseQueue(inputQueue)\n",
    "    inputQueue.put(first)\n",
    "    return inputQueue\n",
    "        "
   ]
  }
