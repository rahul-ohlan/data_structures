    "def removedups(s):\n",
    "    if len(s)==0 or len(s)==1:\n",
    "        return s\n",
    "    if s[0]==s[1]:\n",
    "        output = removedups(s[1:])\n",
    "        return output\n",
    "    else:\n",
    "        output = removedups(s[1:])\n",
    "        return s[0]+output\n",
    "        \n",
    "\n",
    "\n",
    "sting = input()\n",
    "removedups(sting)"
   ]
