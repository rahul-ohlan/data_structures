    "def stairs(n):\n",
    "    if n== 0 or n == 1:\n",
    "        return 1\n",
    "    elif n==2:\n",
    "        return 2\n",
    "    ways = stairs(n-1) + stairs(n-2) + stairs(n-3)\n",
    "    return ways\n",
    "\n",
    "n = int(input())\n",
    "print(stairs(n))"
