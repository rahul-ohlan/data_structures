    "def countZeros(n):\n",
    "    if n==0:\n",
    "        return 1\n",
    "    elif n<10:\n",
    "        return 0\n",
    "    digit = n%10\n",
    "    if digit ==0:\n",
    "        count = 1 + countZeros(n//10)\n",
    "    else:\n",
    "        count = 0 + countZeros(n//10)\n",
    "    return count\n",
    "\n",
    "\n",
    "k = int(input())\n",
    "countZeros(k)"
   ]
