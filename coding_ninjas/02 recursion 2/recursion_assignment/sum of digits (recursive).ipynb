    "def sum_digits(k):\n",
    "    if k==0 or k<10:\n",
    "        return k\n",
    "    unit = k%10\n",
    "    total = unit + sum_digits(k//10)\n",
    "    return total\n",
    "\n",
    "num = int(input())\n",
    "sum_digits(num)"
