   "source": [
    "import heapq\n",
    "\n",
    "def klargest(array,k):\n",
    "    \n",
    "    heap = array[:k]\n",
    "    heapq.heapify(heap)\n",
    "    for element in array[k:]:\n",
    "        if element > heap[0]:\n",
    "            heapq.heapreplace(heap,element)\n",
    "    return heap"
   ]
  },
